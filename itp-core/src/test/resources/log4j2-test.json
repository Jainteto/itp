{
  "configuration": {
    "status": "INFO",
    "appenders": {
      "Console": {
        "name": "Console",
        "target": "SYSTEM_OUT",
        "PatternLayout": {
          "pattern": "%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"
        }
      }
    },
    "Loggers": {
      "Root": {
        "level": "error",
        "AppenderRef": {
          "ref": "Console"
        }
      }
    }
  }
}